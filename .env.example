OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=

# OpenRouter Configuration (for using OpenRouter with Cerebras provider)
# Get your key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=

# Cerebras Configuration (ULTRA-FAST inference)
# Get your key from: https://cloud.cerebras.ai
# Used by: Deep Researcher with Cerebras GLM-4.6
CEREBRAS_API_KEY=
CEREBRAS_MODEL=zai-glm-4.6
# Context: 64K (Free) / 131K (Paid) | Max Output: 40K tokens
CEREBRAS_MAX_OUTPUT_TOKENS=40000
CEREBRAS_TEMPERATURE=1.0

# GitHub Configuration (for Cerebras GitHub Researcher)
# Create a Personal Access Token: https://github.com/settings/tokens
# Required scopes: repo, read:org (for private repos and org access)
GITHUB_TOKEN=
# Optional: Default repository to investigate (format: owner/repo)
GITHUB_REPO=

# BrightData SDK Configuration (Enterprise Web Data Collection)
# Get your API token from: https://brightdata.com/cp/setting/api_token
# When SEARCH_API=brightdata, uses BrightData SDK for Google/Bing search
BRIGHTDATA_API_TOKEN=
SERP_ZONE=serp_api1

# FireCrawl Configuration
# Get your API key from: https://www.firecrawl.dev
# When SEARCH_API=firecrawl, tools are loaded via MCP
FIRECRAWL_API_KEY=

# Perplexity Configuration (Fast & Focused AI Search)
# Get your API key from: https://docs.perplexity.ai
# Best for: technical documentation, debugging, API references
PERPLEXITY_API_KEY=

# Tavily Configuration
TAVILY_API_KEY=

# Search API Configuration
# Options: tavily, firecrawl, brightdata, perplexity, anthropic, openai, none
# - firecrawl: Web scraping with full page content (default for Cerebras)
# - tavily: Comprehensive web search
# - brightdata: Enterprise data collection
# - perplexity: Fast AI-powered search, great for technical queries
# Default: firecrawl (optimized for Cerebras GLM-4.6)
SEARCH_API=firecrawl

LANGSMITH_API_KEY=
LANGSMITH_PROJECT=
LANGSMITH_TRACING=

# Only necessary for Open Agent Platform
SUPABASE_KEY=
SUPABASE_URL=
# Should be set to true for a production deployment on Open Agent Platform. Should be set to false otherwise, such as for local development.
GET_API_KEYS_FROM_CONFIG=false